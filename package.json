{
  "name": "md-parser",
  "version": "0.1.0",
  "description": "A Markdown parser.",
  "main": "dist/parser.js",
  "directories": {
    "example": "examples",
    "lib": "dist",
    "test": "tests"
  },
  "scripts": {
    "build": "npx rollup -c",
    "build:watch": "npx rollup -c -w",
    "test": "npx ts-mocha tests/**/*.spec.ts --require mochawesome/register",
    "test:watch": "npx ts-mocha -w tests/**/*.spec.ts --require mochawesome/register"
  },
  "keywords": [
    "markdown",
    "md",
    "parser",
    "github-markdown"
  ],
